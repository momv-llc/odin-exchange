# ODIN Exchange Frontend Configuration

# API Configuration
# Leave empty or don't set for demo mode (uses mock data)
# Set to your backend URL for production
# VITE_API_URL=https://api.odineco.pro/api/v1

# Demo Mode
# Set to "true" to force demo mode even with API_URL set
# VITE_DEMO_MODE=true

# Telegram Configuration
VITE_TELEGRAM_BOT_URL=https://bot.odineco.pro
VITE_TELEGRAM_GROUP_URL=https://t.me/OdinExchangeSupport

# Analytics (optional)
# VITE_GA_ID=
# VITE_MIXPANEL_TOKEN=

# Feature Flags
VITE_ENABLE_AB_TESTING=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_CHAT=true

# Application Settings
VITE_APP_NAME=ODIN Exchange
VITE_APP_URL=https://ex.odineco.pro

# Optional (Next.js)
# NEXT_PUBLIC_API_URL=http://localhost:3000/api/v1
# NEXT_PUBLIC_API_URL=https://api.odineco.pro/api/v1
# VITE_API_URL=http://localhost:3000/api/v1
# VITE_API_URL=https://api.odineco.pro/api/v1
